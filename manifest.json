{
  //EXTENSION INFOS
  "manifest_version": 2,
  "name": "persistent  player",
  "version": "2.0",
  "description": "Allow the user to persist the youtube player between pages",
  //SCRIPT INJECTION
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": ["playerPersist.js"],
      "run_at": "document_end"
    }
  ]
}